<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="yh.yhwy.dao.IUserDao">

    <!--通过培训表编码查询培训记录-->
    <select id="selectUserByUserid" resultType="User">
          select *
          from user
          where userid = #{userid}
    </select>
</mapper>